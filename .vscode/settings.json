{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.keystyle": "nested",
  "cSpell.words": ["Dropzone", "ulid"]
}
